{
  "name": "@yxonic/vmark",
  "version": "0.0.7",
  "author": "Yu Yin <yxonic@gmail.com>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist/ && tsc && tsc -p tsconfig.cjs.json",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx,.vue src",
    "tc": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "markdown-it": "^12.2.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-cjk-breaks": "^1.1.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-custom-block": "^0.1.2",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vue/server-renderer": "^3.2.23",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.6.14"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
